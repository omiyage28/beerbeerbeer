<table class="rakuten_image">
 <thead>
  <tr>
   <th>ITEM LISTs</th>
  </tr>
 </thead>
<tbody>
  <% if @items.present? %>
   <% @items.first(5).each do |item| %>
    <tr>
      <td ><img src="<%= item['smallImageUrls'][0] %>"></td>
      <td><%= link_to item.name, "#{item.url}" %></td>
    </tr>
  <% end %>
 <% end %>
</tbody>
</table>