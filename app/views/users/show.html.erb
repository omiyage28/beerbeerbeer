<div class="main">
  <div class="inner">
    <div class="user__wrapper">
    
      <table class="table">
        <tbody>
          <tr>
            <th class="table__col1">NAME</th>
            <td class="table__col2"><%= @user.name  %></td>
          </tr>
        </tbody>
      </table>
      <h2 class="page-heading">
        <%= "#{@user.name} POSTs"%>
      </h2>
      <div class="user__card">
       <%= render partial: "images/image", collection: @images %>
      </div>
    </div>
  </div>
</div>

 <% if user_signed_in? && @user == current_user %>
  <% unless @user.email == "test@example.com" then %>
     <div class="mt-3">
        <%= link_to "CANCEL THE MEMBERSHIP",user_path(current_user.id), class: "destroy-user", method: :delete, "data-confirm" => "本当に退会しますか？" %>
     </div>
  <% end %>
<% end %>